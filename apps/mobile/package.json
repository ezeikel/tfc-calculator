{
  "name": "@tfc-calculator/mobile",
  "private": true,
  "version": "0.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "clean": "git clean -xdf .expo node_modules",
    "start": "expo start",
    "dev": "expo start --ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "build:ios": "pnpm prebuild:ios && pnpm ios",
    "build:android": "pnpm prebuild:android && pnpm android",
    "build": "expo export",
    "eas:build:local": "eas build --profile development --local",
    "eas:build:local:android": "eas build --platform android --profile preview --local",
    "eas:build:local:ios": "eas build --platform ios --profile preview --local",
    "eas:build:development": "eas build --profile development",
    "eas:build:development:android": "eas build --platform android --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:preview:ios": "eas build --profile preview --platform ios",
    "eas:build:preview:android": "eas build --profile preview --platform android",
    "eas:build:preview:ios:local": "eas build --profile preview --platform ios --local",
    "eas:build:preview:android:local": "eas build --profile preview --platform android --local",
    "eas:build:production": "eas build --profile production",
    "eas:build:production:local": "eas build --profile production --local",
    "eas:submit:beta": "eas submit --profile beta",
    "eas:submit:production": "eas submit --profile production",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "check-types": "tsc --noEmit",
    "env:pull": "eas env:pull --environment development"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@expo-google-fonts/public-sans": "^0.4.2",
    "@expo-google-fonts/source-sans-pro": "^0.2.3",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/pro-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-native-fontawesome": "^0.3.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@sentry/react-native": "^7.2.0",
    "@tanstack/react-form": "^1.23.4",
    "expo": "~54.0.12",
    "expo-application": "~7.0.7",
    "expo-constants": "^18.0.9",
    "expo-dev-client": "~6.0.13",
    "expo-device": "~8.0.9",
    "expo-file-system": "^19.0.16",
    "expo-font": "^14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-linking": "^8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "^6.0.10",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "nativewind": "5.0.0-preview.1",
    "posthog-react-native": "^4.7.1",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-css": "^3.0.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-google-mobile-ads": "^15.8.0",
    "react-native-purchases": "^9.5.3",
    "react-native-purchases-ui": "^9.5.3",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.14",
    "eslint-config-expo": "~10.0.0",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2"
  }
}
